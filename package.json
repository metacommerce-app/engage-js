{
  "name": "@metacommerce-app/engage-js",
  "version": "0.0.1",
  "description": "Metacommerce Engage Javascript SDK",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rimraf -g ./lib",
    "build": "npm run clean && npm run build:cjs",
    "build:cjs": "tsc -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand --detectOpenHandles",
    "watch": "tsc -w",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metacommerce-app/engage-js.git"
  },
  "author": "Metacommerce Admin <it-admin@metacommerce.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metacommerce-app/engage-js/issues"
  },
  "homepage": "https://github.com/metacommerce-app/engage-js#readme",
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@commitlint/types": "19.5.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.1",
    "@types/node-fetch": "2.6.11",
    "@types/prettier": "3.0.0",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "dotenv": "16.4.5",
    "esbuild": "0.24.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "rimraf": "5.0.10",
    "semantic-release": "24.2.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  }
}
