{
  "name": "@metacommerce-app/engage-js",
  "version": "0.0.1",
  "description": "Metacommerce Engage Javascript SDK",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rimraf -g ./lib",
    "build": "npm run clean && npm run build:cjs",
    "build:cjs": "tsc -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand --detectOpenHandles",
    "watch": "tsc -w",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metacommerce-app/engage-js.git"
  },
  "author": "Metacommerce Admin <it-admin@metacommerce.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metacommerce-app/engage-js/issues"
  },
  "homepage": "https://github.com/metacommerce-app/engage-js#readme",
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@commitlint/types": "19.8.1",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.1",
    "@types/node-fetch": "2.6.13",
    "@types/prettier": "3.0.0",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "dotenv": "17.2.2",
    "esbuild": "0.25.9",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.1.3",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.7",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
